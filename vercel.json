{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "^/css/(.*)", "dest": "/csss/$1" },
    { "src": "^/js/(.*)", "dest": "/jss/$1" },
    { "src": "^/site.webmanifest$", "dest": "/site.webmanifest" },
    { "src": "^/(.*)$", "dest": "/$1" }
  ]
}
