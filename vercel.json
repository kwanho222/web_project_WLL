{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "htmls/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "^/htmls/(.*)", "dest": "/htmls/$1" },
    { "src": "^/csss/(.*)", "dest": "/csss/$1" },
    { "src": "^/jss/(.*)", "dest": "/jss/$1" },
    { "src": "^/img/(.*)", "dest": "/img/$1" },
    { "src": "^/$", "dest": "/index.html" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
